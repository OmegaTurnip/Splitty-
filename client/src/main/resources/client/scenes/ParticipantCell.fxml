<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="17.0" prefWidth="179.0" styleClass="participant-cell" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.ParticipantCellController">
   <children>
      <HBox alignment="CENTER" prefHeight="54.0" prefWidth="155.0">
         <children>
            <Label fx:id="participantCellLabel" alignment="CENTER" prefHeight="34.0" prefWidth="106.0" text="Label">
               <font>
                  <Font name="Arial" size="17.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="110.0">
               <children>
                  <Button fx:id="editParticipantButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="38.0" styleClass="button-edit">
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="deleteParticipantButton" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="38.0" styleClass="button-delete">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</StackPane>
