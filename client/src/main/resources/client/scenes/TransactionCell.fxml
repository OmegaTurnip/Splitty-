<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="17.0" prefWidth="179.0" styleClass="event-cell" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.TransactionCellController">
   <children>
      <HBox alignment="CENTER" prefHeight="22.0" prefWidth="190.0">
         <children>
            <Label id="transactionInfoLabel" fx:id="transactionInfoLabel" alignment="CENTER" prefHeight="34.0" prefWidth="106.0" styleClass="list-cell" text="Label" textFill="#fefdfd">
               <font>
                  <Font name="Garamond" size="9.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="110.0">
               <children>
                  <Button id="editTransactionButton" fx:id="editTransactionButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="38.0" styleClass="button-edit">
                     <font>
                        <Font name="Garamond" size="12.0" />
                     </font>
                  </Button>
                  <Button id="deleteTransactionButton" fx:id="deleteTransactionButton" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="38.0" styleClass="button-delete">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Garamond" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</StackPane>
