<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="44.0" styleClass="event-cell" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.TransactionCellController">
   <HBox alignment="CENTER" prefHeight="22.0" prefWidth="190.0">
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label id="transactionInfoLabel" fx:id="transactionInfoLabel" graphicTextGap="0.0" prefHeight="22.0" prefWidth="318.0" styleClass="list-cell" stylesheets="@style.css" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="35.0" prefWidth="110.0" HBox.hgrow="NEVER">
         <Button id="editTransactionButton" fx:id="editTransactionButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="38.0" styleClass="button-edit">
            <font>
               <Font name="Arial" size="12.0" />
            </font>
         </Button>
         <Button id="deleteTransactionButton" fx:id="deleteTransactionButton" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="38.0" styleClass="button-delete">
            <HBox.margin>
               <Insets left="10.0" />
            </HBox.margin>
            <font>
               <Font name="Arial" size="12.0" />
            </font>
         </Button>
      </HBox>
   </HBox>
</StackPane>
